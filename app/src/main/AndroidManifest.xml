<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.dks.restbee">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/SplashTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".WelcomeActivity"
            android:theme="@style/Theme.AppCompat.DayNight" />
        <activity android:name=".LoginStudentActivity"
            android:theme="@style/Theme.AppCompat.DayNight" />
        <activity android:name=".LoginAdminActivity"
            android:theme="@style/Theme.AppCompat.DayNight" />
        <activity android:name=".RegisterAdminActivity"
            android:theme="@style/Theme.AppCompat.DayNight" />
        <activity android:name=".WelcomeStudent"
            android:theme="@style/Theme.AppCompat.DayNight" />
        <activity android:name=".WelcomeAdmin"
            android:theme="@style/Theme.AppCompat.DayNight" />
    </application>

</manifest>